<?xml version="1.0"?>
<Options>
<!--Description of menu items with commands name-->
<!--Menu items will be built using command descriptions if they are not popup-->
<!--Otherwise the command name specifies the real menu item label-->
<!--Separator are specified using SEPARATOR string as command name-->
  <Class AF_Name="MenuItem">
    <label AF_Class="string" AF_Value=""/>
    <isPopup AF_Class="bool" AF_Value="false"/>
    <popupItems AF_Class="MenuItem" AF_IsArray="1"/>
  </Class>
  <Class AF_Name="Menu">
    <items AF_Class="MenuItem" AF_IsArray="1"/>
  </Class>
  <Class AF_Name="Toolbar">
    <Name AF_Class="string" AF_Value=""/>
    <InitialeDockingSide AF_Class="string" AF_Value="Top"/>
    <IsUserBar AF_Class="bool" AF_Value="false"/>
    <DoNotUse AF_Class="bool" AF_Value="false"/>
    <items AF_Class="MenuItem" AF_IsArray="1"/>
  </Class>
  <Class AF_Name="PopupMenu">
    <AF_Superclasses>
      <AF_Superclasses AF_Classname="Toolbar"/>
    </AF_Superclasses>
  </Class>
<!--***************************************************************************-->
<!--Menu Variables-->
  <Toolbar AF_Name="MainMenu" InitialeDockingSide="Top" IsUserBar="false" DoNotUse="false" Name="&amp;MainMenu">
    <items>
