command NewGrapherBuffer {
    label "&newGrapherBuffer";
    prompt "&newGrapherBufferP"
    category buffer;
    category grapher;
    bitmap "ivstudio/grapher/icgrapher.png"
    // this command execute the NewBuffer command
}

